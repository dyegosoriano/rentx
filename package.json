{
  "name": "rentx-api",
  "version": "1.0.0",
  "main": "dist/shared/infra/http/server.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn --clear src/shared/infra/http/server.ts",
    "start": "NODE_ENV=production yarn typeorm migration:run && node dist/shared/infra/http/server.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watch --maxWorkers=25%",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "seed:admin": "ts-node-dev src/shared/infra/typeorm/seed/admin.ts",
    "db:migrate:make": "yarn typeorm migration:create -n",
    "db:migrate:revert": "yarn typeorm migration:revert",
    "db:migrate": "yarn typeorm migration:run"
  },
  "dependencies": {
    "aws-sdk": "^2.1058.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "csv-parse": "^4.16.3",
    "dayjs": "^1.10.7",
    "dotenv": "^12.0.4",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "multer": "^1.4.3",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.10",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/preset-env": "^7.16.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mime": "^2.0.3",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2"
  }
}
